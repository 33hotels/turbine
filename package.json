{
  "name": "@funkia/funnel",
  "version": "0.0.6",
  "description": "A functional frontend framework in Typescript",
  "main": "dist/cmjs/index.js",
  "jsnext:main": "dist/es6/index.js",
  "typings": "dist/defs/index.d.ts",
  "directories": {
    "dist": "dist"
  },
  "scripts": {
    "test": "./node_modules/.bin/karma start karma-ts.conf.js",
    "build-web": "./node_modules/.bin/webpack",
    "build-es6": "./node_modules/.bin/tsc -P tsconfig.json",
    "build-cmjs": "./node_modules/.bin/tsc -P tsconfig-cmjs.json",
    "build": "npm run build-web; npm run build-es6; npm run build-cmjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Funkia/funnel.git"
  },
  "keywords": [
    "functional",
    "framework",
    "typescript"
  ],
  "license": "MIT",
  "peerDependencies": {
    "jabz": "^0.0.12",
    "hareactive": "^0.0.8"
  },
  "bugs": {
    "url": "https://github.com/Funkia/funnel/issues"
  },
  "homepage": "https://github.com/Funkia/funnel#readme",
  "devDependencies": {
    "@types/chai": "^3.4.33",
    "@types/mocha": "^2.2.32",
    "chai": "^3.5.0",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-mocha": "^1.2.0",
    "karma-typescript": "^2.1.5",
    "mocha": "^3.1.0",
    "ts-loader": "^1.3.3",
    "typescript": "^2.0.3",
    "webpack": "^1.14.0"
  },
  "contributors": [
